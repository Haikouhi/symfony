{% extends "base.html.twig" %}


{% block body %}
	<h1>Hello world</h1>

	{% for produit in produits %}

		<div class="card">
			<div class="card-header"> {{ produit.titre }} </div>

			<div class="card-body">
				Note: {{ produit.note}} <br>

				<p>
					{{ produit.description }}
				</p>
				
			</div>
		</div>

	{% endfor %}

{% endblock %}